    "query": {
        "from": {
            "table": "Surveys\\Surveys\\Users\\Code\\Tables\\Users",
            "alias": "ssu",
            "name": "#__surveys_surveys_users"
        },
        "select": [
            "ssu.id",
            "ssu.survey_id",
            "ss.title AS survey_id_title",
            "ssu.user_id",
            "uu.name AS user_id_name",
            "ssu.guest_id",
            "ssu.created_by",
            "uu_8279.name AS created_by_name",
            "ssu.date_created",
            "ssu.modified_by",
            "uu_8817.name AS modified_by_name",
            "ssu.date_modified"
        ],
        "leftjoin": [
            {
                "table": "Surveys\\Surveys\\Code\\Tables\\Surveys",
                "alias": "ss",
                "name": "#__surveys_surveys",
                "on": "ssu.survey_id = ss.id"
            },
            {
                "table": "Users\\Users\\Code\\Tables\\Users",
                "alias": "uu",
                "name": "#__users_users",
                "on": "ssu.user_id = uu.id"
            },
            {
                "table": "Users\\Users\\Code\\Tables\\Users",
                "alias": "uu_8279",
                "name": "#__users_users",
                "on": "ssu.created_by = uu_8279.id"
            },
            {
                "table": "Users\\Users\\Code\\Tables\\Users",
                "alias": "uu_8817",
                "name": "#__users_users",
                "on": "ssu.modified_by = uu_8817.id"
            }
        ]

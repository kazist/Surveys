    "query": {
        "from": {
            "table": "Surveys\\Surveys\\Questions\\Code\\Tables\\Questions",
            "alias": "ssq",
            "name": "#__surveys_surveys_questions"
        },
        "select": [
            "ssq.id",
            "ssq.survey_id",
            "ss.title AS survey_id_title",
            "ssq.question_id",
            "sq.question AS question_id_question",
            "ssq.created_by",
            "uu.name AS created_by_name",
            "ssq.date_created",
            "ssq.modified_by",
            "uu_2993.name AS modified_by_name",
            "ssq.date_modified"
        ],
        "leftjoin": [
            {
                "table": "Surveys\\Surveys\\Code\\Tables\\Surveys",
                "alias": "ss",
                "name": "#__surveys_surveys",
                "on": "ssq.survey_id = ss.id"
            },
            {
                "table": "Surveys\\Questions\\Code\\Tables\\Questions",
                "alias": "sq",
                "name": "#__surveys_questions",
                "on": "ssq.question_id = sq.id"
            },
            {
                "table": "Users\\Users\\Code\\Tables\\Users",
                "alias": "uu",
                "name": "#__users_users",
                "on": "ssq.created_by = uu.id"
            },
            {
                "table": "Users\\Users\\Code\\Tables\\Users",
                "alias": "uu_2993",
                "name": "#__users_users",
                "on": "ssq.modified_by = uu_2993.id"
            }
        ]
    }

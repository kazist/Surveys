    "query": {
        "from": {
            "table": "Surveys\\Surveys\\Results\\Code\\Tables\\Results",
            "alias": "ssr",
            "name": "#__surveys_surveys_results"
        },
        "select": [
            "ssr.id",
            "ssr.question_id",
            "sq.question AS question_id_question",
            "ssr.survey_user_id",
            "ssu.survey_id AS survey_user_id_survey_id",
            "ssu.user_id AS survey_user_id_user_id",
            "ssu.guest_id AS survey_user_id_guest_id",
            "ssr.user_id",
            "uu.name AS user_id_name",
            "ssr.guest_id",
            "ssr.result",
            "ssr.created_by",
            "uu_2010.name AS created_by_name",
            "ssr.date_created",
            "ssr.modified_by",
            "uu_3798.name AS modified_by_name",
            "ssr.date_modified"
        ],
        "leftjoin": [
            {
                "table": "Surveys\\Questions\\Code\\Tables\\Questions",
                "alias": "sq",
                "name": "#__surveys_questions",
                "on": "ssr.question_id = sq.id"
            },
            {
                "table": "Surveys\\Surveys\\Users\\Code\\Tables\\Users",
                "alias": "ssu",
                "name": "#__surveys_surveys_users",
                "on": "ssr.survey_user_id = ssu.id"
            },
            {
                "table": "Users\\Users\\Code\\Tables\\Users",
                "alias": "uu",
                "name": "#__users_users",
                "on": "ssr.user_id = uu.id"
            },
            {
                "table": "Users\\Users\\Code\\Tables\\Users",
                "alias": "uu_2010",
                "name": "#__users_users",
                "on": "ssr.created_by = uu_2010.id"
            },
            {
                "table": "Users\\Users\\Code\\Tables\\Users",
                "alias": "uu_3798",
                "name": "#__users_users",
                "on": "ssr.modified_by = uu_3798.id"
            }
        ]
    }

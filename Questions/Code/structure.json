    "query": {
        "from": {
            "table": "Surveys\\Questions\\Code\\Tables\\Questions",
            "alias": "sq",
            "name": "#__surveys_questions"
        },
        "select": [
            "sq.id",
            "sq.question",
            "sq.type",
            "sq.chart",
            "sq.ordering",
            "sq.created_by",
            "uu.name AS created_by_name",
            "sq.date_created",
            "sq.modified_by",
            "uu_5010.name AS modified_by_name",
            "sq.date_modified"
        ],
        "leftjoin": [
            {
                "table": "Users\\Users\\Code\\Tables\\Users",
                "alias": "uu",
                "name": "#__users_users",
                "on": "sq.created_by = uu.id"
            },
            {
                "table": "Users\\Users\\Code\\Tables\\Users",
                "alias": "uu_5010",
                "name": "#__users_users",
                "on": "sq.modified_by = uu_5010.id"
            }
        ]
    }
